{
  "name": "snapapp_admin",
  "version": "1.0.0",
  "description": "Admin UI for Snapapp.pro. ",
  "author": "Nathan Briar Davis",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
