    "jest": "23.6.0",
    "jest-serializer-vue": "2.0.2",
    "jest-vue-preprocessor": "1.4.0",
    "jsdom": "12.2.0",
    "nodemon": "1.18.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "3.0.0",
    "vue-server-renderer": "2.5.17"
