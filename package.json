    "js-cookie": "2.2.0",
    "jwt-decode": "2.2.0",
    "morgan": "1.9.1",
    "nuxt": "^2.0.0",
    "uuid": "3.3.2",
    "vee-validate": "2.1.0-beta.11",
    "vuetify": "1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-proposal-function-sent": "7.1.0",
